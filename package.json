{
  "name": "template",
  "version": "0.0.2",
  "description": "A Node Typescript template",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "lint": "eslint src",
    "watch": "tsc -w --pretty",
    "start": "node dist/index.js",
    "build": "rimraf ./dist && tsc --build",
    "prettier": "prettier --write src/**/*.{js,ts,json}"
  },
  "engines": {
    "node": ">=14.x",
    "npm": ">=7.x"
  },
  "keywords": [
    "Eslint",
    "Typescript",
    "Nodejs",
    "Nodemon"
  ],
  "author": "ChampionBuffalo1 <primarymail@duck.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/node": "^20.11.9",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "pretty-quick": "^4.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {}
}
